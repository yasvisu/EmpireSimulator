<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="554.0" prefWidth="704.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.IntroMenuController">
   <top>
      <Pane fx:id="IntroMenuTitlePane" prefHeight="109.0" prefWidth="704.0" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <BorderPane fx:id="IntroMenuBorderPane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <bottom>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="80.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="IntroMenuOptions" mnemonicParsing="false" onAction="#IntroMenuOptions_onAction" text="Options" />
                  <Button fx:id="IntroMenuCredits" mnemonicParsing="false" onAction="#IntroMenuCredits_onAction" text="Credits" />
                  <Button fx:id="IntroMenuAbout" mnemonicParsing="false" onAction="#IntroMenuAbout_onAction" text="About" />
               </children>
            </HBox>
         </bottom>
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="30.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="IntroMenuClickLabel" text="Click Label" />
                  <Button fx:id="IntroMenuNew" mnemonicParsing="false" onAction="#IntroMenuNew_onAction" text="New" />
                  <Button fx:id="IntroMenuLoad" mnemonicParsing="false" onAction="#IntroMenuLoad_onAction" text="Load" />
                  <Button fx:id="IntroMenuHighscores" mnemonicParsing="false" onAction="#IntroMenuHighscores_onAction" text="High-scores" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </center>
</BorderPane>
